<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:u="urn:import:gwtupload.client">
	<g:HTMLPanel ui:field="container">
		<g:DialogBox ui:field="dialogBox">
			<g:HTMLPanel>
				<div id="main">
					<article class="module width_full">
						<header>
							<h3>Upload image</h3>
						</header>
						<div class="module_content">
						
							<g:FormPanel ui:field="form">
								<u:MultiUploader ui:field="uploader"></u:MultiUploader>
							</g:FormPanel>
							
							<g:HTML ui:field="errorMsg"></g:HTML>
							
						</div>

						<footer>
							<div class="submit_link">
								<g:SubmitButton ui:field="uploadBtn" styleName="alt_btn">Upload</g:SubmitButton>
								<g:SubmitButton ui:field="cancelBtn">Cancel</g:SubmitButton>
							</div>
						</footer>

					</article>
				</div>
			</g:HTMLPanel>
		</g:DialogBox>
	</g:HTMLPanel>
</ui:UiBinder> 